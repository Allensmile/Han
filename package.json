{
  "name": "han-css",
  "version": "3.0.2",
  "description": "The CSS typography framework optimised for Hanzi",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethantw/Han.git"
  },
  "main": "han.js",
  "keywords": [
    "Hanzi",
    "CJK",
    "normalize",
    "typography",
    "typesetting"
  ],
  "author": {
    "name": "Chen Yijun (@ethantw)",
    "url": "http://yijun.me/"
  },
  "license": "MIT Lisence",
  "bugs": {
    "url": "https://github.com/ethantw/Han/issues"
  },
  "scripts": {
    "test": "grunt",
    "build": "grunt",
    "build:libsass": "grunt sass",
    "build:gemsass": "sass --sourcemap=none sass/han.sass:han.css --style expanded",
    "watch:gemsass": "sass --watch --sourcemap=none sass/han.sass:han.css --style expanded",
    "build:js": "grunt dev"
  },
  "dependencies": {
    "normalize.css": "~3.0.2"
  },
  "devDependencies": {
    "grunt": "~0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-contrib-cssmin": "^0.10.0",
    "grunt-contrib-jshint": "0.8.0",
    "grunt-contrib-uglify": "0.3.2",
    "grunt-jsonlint": "1.0.4",
    "grunt-sass": "^0.17.0",
    "gzip-js": "0.3.2",
    "load-grunt-tasks": "0.3.0",
    "node-sass": "^1.2.3",
    "requirejs": "2.1.10"
  }
}
